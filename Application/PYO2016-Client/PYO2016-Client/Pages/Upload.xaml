<UserControl x:Class="PYO2016_Client.Pages.BasicPage1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".1*" />
            <ColumnDefinition Width=".8*" />
            <ColumnDefinition Width=".1*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height=".3*" />
        </Grid.RowDefinitions>
        <ScrollViewer>
            <StackPanel MinWidth="200"/>
        </ScrollViewer>
        <TextBlock x:Name="textBlock" Grid.Column="1" Margin="0,0,0,0" Text="Files" VerticalAlignment="Top" FontFamily="a옛날목욕탕L 보통" FontSize="30"/>
        <Button x:Name="captureButton" FontFamily="a옛날목욕탕L 보통" Content="Capture" HorizontalAlignment="Right" Height="32" Margin="0,0,0,0" VerticalAlignment="Top" Width="94" Grid.Column="1" Click="captureButton_Click_1"/>
        <ListView x:Name="listView" Grid.Column="1" Grid.Row="0" Margin="0,43,0,151" FontFamily="a옛날목욕탕L 보통">
            <ListViewItem Content="ListViewItem"/>
            <ListViewItem Content="ListViewItem"/>
            <ListViewItem Content="ListViewItem"/>
            <ListViewItem Content="ListViewItem"/>
        </ListView>
        <Button x:Name="button" FontFamily="a옛날목욕탕L 보통" Content="Add" HorizontalAlignment="Center" Height="32" Margin="-140,133,0,0" VerticalAlignment="Top" Width="94" Grid.Column="1"/>
        <Button x:Name="button1" FontFamily="a옛날목욕탕L 보통" Content="Remove" HorizontalAlignment="Center" Height="32" Margin="80,133,0,0" VerticalAlignment="Top" Width="94" Grid.Column="1"/>
        <Button x:Name="button2" FontFamily="a옛날목욕탕L 보통" Content="Analysis" HorizontalAlignment="Center" Height="32" Margin="-20,203,0,0" VerticalAlignment="Top" Width="94" Grid.Column="1"/>
    </Grid>
</UserControl>
